def Insertion_Sort(l):
    if l==[]:
        return l
    elif len(l)==1:
        return l   
    length=len(l)
    
    for i in range(1,length):
        tmp=l[i]
        for j in range(i-1,-1,-1):
            if tmp<l[j]:
                l[j+1]=l[j]
            else:
                break
        if j==0 and tmp<l[0]:
            l[0]=tmp
        else:
            
            l[j+1]=tmp
    return l
